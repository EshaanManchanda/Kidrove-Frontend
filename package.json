{
  "name": "kidzapp-clone-frontend",
  "version": "1.0.2",
  "description": "Frontend for KidzApp Clone - Kids Activities Platform",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "node -e \"console.log('Node:', process.version); console.log('Vite available:', require('fs').existsSync('./node_modules/.bin/vite') ? 'Yes' : 'No')\"",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "vercel:build": "npm run type-check && npm run lint && npm run build",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules && npm install",
    "health-check": "node -e \"console.log('Health check passed'); process.exit(0)\"",
    "postbuild": "node -e \"const fs = require('fs'); const path = 'dist/index.html'; if (fs.existsSync(path)) { console.log('‚úÖ Build successful - index.html exists'); console.log('üì¶ Build size:', fs.statSync('dist').size + ' bytes'); } else { console.error('‚ùå Build failed - index.html not found'); process.exit(1); }\""
  },

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Gema
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Event Management and Booking Platform
VITE_APP_URL=http://localhost:5173

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_REFRESH_TOKEN_EXPIRY=7d
VITE_ACCESS_TOKEN_EXPIRY=15m

# Payment Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Map Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,image/gif
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Social Media
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_TWITTER_API_KEY=your_twitter_api_key

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_FACEBOOK_PIXEL_ID=your_facebook_pixel_id

# Email Services
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Push Notifications
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Development
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_API=false
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_FAVORITES=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_VENDOR_REGISTRATION=true
VITE_ENABLE_MULTI_LANGUAGE=true
VITE_ENABLE_DARK_MODE=true

# Localization
VITE_DEFAULT_LANGUAGE=en
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_TIMEZONE=UTC
VITE_SUPPORTED_LANGUAGES=en,es,fr,de,ar
VITE_SUPPORTED_CURRENCIES=USD,EUR,GBP,CAD,AUD

# SEO
VITE_SITE_NAME=Gema - Event Management Platform
VITE_SITE_DESCRIPTION=Discover and book amazing events in your area
VITE_SITE_KEYWORDS=events,booking,tickets,entertainment,workshops,conferences
VITE_SITE_AUTHOR=Gema Team
VITE_SITE_IMAGE=https://your-domain.com/og-image.jpg

# Contact Information
VITE_CONTACT_EMAIL=contact@gema.com
VITE_SUPPORT_EMAIL=support@gema.com
VITE_CONTACT_PHONE=+1-555-0123
VITE_COMPANY_ADDRESS=123 Main St, City, State 12345

# Legal
VITE_PRIVACY_POLICY_URL=/privacy
VITE_TERMS_OF_SERVICE_URL=/terms
VITE_COOKIE_POLICY_URL=/cookies

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_STATIC_CACHE_DURATION=86400000

# Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=900000

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_REDIRECT=true
VITE_SECURE_COOKIES=true